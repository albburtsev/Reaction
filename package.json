    "sinon": "^1.17.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "ts-loader": "^0.9.5",
    "typescript": "^2.0.3",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0"
  }
