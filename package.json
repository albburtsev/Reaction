{
  "name": "reaction",
  "version": "0.0.0",
  "description": "React/Redux/ReactRouter starter kit",
  "main": "index.js",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "lint": "eslint src/ test/ --ext .js --ext .jsx",
    "build": "NODE_ENV=production webpack --config=webpack.config.prod.js",
    "build-info": "NODE_ENV=production webpack --colors --display-modules --config=webpack.config.prod.js",
    "mocha": "NODE_PATH=$NODE_PATH:$PWD/src mocha test --watch src/**/*.spec.js",
    "test": "NODE_PATH=$NODE_PATH:$PWD/src mocha test src/**/*.spec.js && yarn run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albburtsev/reaction.git"
  },
  "keywords": [
    "React",
    "Flux",
    "App"
  ],
  "author": "Alexander Burtsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/albburtsev/reaction/issues"
  },
  "homepage": "https://github.com/albburtsev/reaction#readme",
  "dependencies": {
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.18",
    "classnames": "^2.2.5",
    "leaflet": "1.0.0-beta.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-syntax-class-properties": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.6.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.5.0",
    "chalk": "^1.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.6.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "http-proxy-middleware": "^0.17.3",
