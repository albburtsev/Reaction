{
  "name": "reaction",
  "version": "0.0.0",
  "description": "React/Redux/ReactRouter starter kit",
  "main": "index.js",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "lint:es": "eslint src/ test/ --ext .js --ext .jsx",
    "lint:ts": "tslint '{src,test}/**/*.{ts,tsx}' -e 'src/**/*.d.ts' -- --project=tsconfig.json",
    "lint": "npm run lint:es & npm run lint:ts",
    "build": "NODE_ENV=production webpack --config=webpack.config.prod.js",
    "build-info": "NODE_ENV=production webpack --colors --display-modules --config=webpack.config.prod.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "precommit": "npm run lint",
    "prepush": "npm run lint & npm test",
    "clean": "rm -rf build/ coverage/ demo/",
    "demo": "bash scripts/demo.sh"
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js|jsx)$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albburtsev/reaction.git"
  },
  "keywords": [
    "React",
    "Flux",
    "App"
  ],
  "author": "Alexander Burtsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/albburtsev/reaction/issues"
  },
  "homepage": "https://github.com/albburtsev/reaction#readme",
  "dependencies": {
    "@types/classnames": "^2.2.0",
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.18",
    "@types/react-redux": "^4.4.41",
    "@types/react-router-dom": "^4.0.4",
    "babel-jest": "^19.0.0",
    "classnames": "^2.2.5",
    "leaflet": "1.0.0-beta.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.4",
    "awesome-typescript-loader": "^3.1.3",
    "babel-core": "^6.3.21",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-syntax-class-properties": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.6.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chalk": "^1.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.6.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "http-proxy-middleware": "^0.17.3",
    "husky": "^0.13.4",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.2",
    "react-dev-utils": "^0.4.1",
    "react-docgen-typescript": "^0.0.11",
    "react-styleguidist": "^5.4.4",
    "redux-devtools-extension": "^2.13.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^20.0.5",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "typings-for-css-modules-loader": "^1.5.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0"
  }
}
